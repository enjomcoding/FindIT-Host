{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { 
          "runtime": "vercel-php@0.7.4"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|fonts|webfonts|storage|favicon.ico|robots.txt|manifest.webmanifest|manifest.json|.*\\..*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr"
    }
}